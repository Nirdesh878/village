            },
            }],
            yAxes: [{
            display: true,
            scaleLabel: {
            display: false,
            labelString: 'Index Returns'
            },
            ticks: {
            min: 0,
            // forces step size to be 5 units
            stepSize: 0.5
            }
            }]
            }
            }
            };
        
        
            var ctx1 = document.getElementById('rate_line').getContext('2d');
            window.myLine = new Chart(ctx1, config);
